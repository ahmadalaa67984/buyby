{
  "name": "buyby",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=7.24.0"
  },
  "scripts": {
    "migrate-up": "npx migrate-mongo up",
    "prebuild": "npm run migrate-up && rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run migrate-up && node dist/src/main.js",
    "start:heroku": "npm run migrate-up && node dist/myApp/server/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV='production' node dist/src/main && npm run migrate-up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "stop-win": "Taskkill /IM node.exe /F",
    "stop-linux": "killall node",
    "seed": "ts-node -r tsconfig-paths/register database/seed.js",
    "postinstall": "node scripts/postinstall.js"
  },
